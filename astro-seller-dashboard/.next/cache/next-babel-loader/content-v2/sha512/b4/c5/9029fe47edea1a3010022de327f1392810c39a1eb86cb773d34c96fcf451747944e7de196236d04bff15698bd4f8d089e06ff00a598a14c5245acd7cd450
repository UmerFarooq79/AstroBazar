n      .then((data) => {\r\n        // Handle the response from the server\r\n        console.log(data['username'])\r\n        const userData = data['id']\r\n        if (data['username']) {\r\n          // Login was successful, redirect the user to the home page\r\n          router.push('/home');\r\n          router.push({ pathname: '..products/create-product', query: { user: data } });\r\n        } else {\r\n          // Show an error message\r\n          alert(\"error\");\r\n        }\r\n      });\r\n  };\r\n\r\n  // Event handler for when the input values are changed\r\n  const handleChange = (event) => {\r\n    setFormData({\r\n      ...formData,\r\n      [event.target.name]: event.target.value\r\n    });\r\n  };\r\n\r\n  return (\r\n    <UserContext.Provider value={userData}>\r\n    <form onSubmit={handleSubmit}>\r\n      <label htmlFor=\"username\">Username:</label>\r\n      <input type=\"text\" name=\"username\" value={formData.username} onChange={handleChange} />\r\n\r\n      <label htmlFor=\"password\">Password:</label>\r\n      <input type=\"password\" name=\"password\" value={formData.password} onChange={handleChange} />\r\n\r\n      <button type=\"submit\">Log In</button>\r\n      <Link href='/register'> Don't have an account? Sigh Up </Link>\r\n    </form>\r\n    </UserContext.Provider>\r\n  );\r\n};\r\n\r\nexport default LoginForm;\r\n"]},"metadata":{},"sourceType":"module"}